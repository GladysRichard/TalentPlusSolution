@page "/"
@inject HttpClient Http

<PageTitle>Index</PageTitle>

<h1>@Display</h1>


 @code{
    public string? Display { get; set; }

    protected TalentPlus? TalentPlus = new();

    protected override async Task OnInitializedAsync()
    {
     
        TalentPlus = await Http.GetFromJsonAsync<TalentPlus>("api/TalentPlus");
        Display = TalentPlus.Title;
    }
 }
